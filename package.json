    "express": "^4.13.4",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-api-middleware": "^1.0.2",
    "redux-thunk": "^2.1.0"
  },
