{
  "name": "webpack-simple-starter",
  "version": "1.1.0",
  "description": "Simple stater for webpack built with Redux",
  "main": "index.js",
  "scripts": {
    "test": "karma start --env==test",
    "test:watch": "karma start --env==test --autoWatch=true --singleRun=false",
    "start": "webpack-dev-server --env=dev",
    "build": "npm run lint && npm run clean && webpack --env=prod",
    "server": "npm run build && node index.js",
    "lint": "eslint --ext .js,.jsx .",
    "clean": "rimraf public/*"
  },
  "keywords": [
    "React",
    "Webpack",
    "React boilerplate"
  ],
  "author": "Phuc Nguyen Hoang",
  "license": "MIT",
  "dependencies": {
